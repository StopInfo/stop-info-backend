{
  "name": "stop-info-backend",
  "version": "0.0.1",
  "description": "backend for StopInfo applications",
  "main": "dist/server.js",
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "build": "tsc",
    "test": "jest",
    "start": "ts-node src/server.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rm -rf dist && npm run build && cp -r src/public dist/public",
    "start:prod": "node dist/server.js"
  },
  "keywords": [
    "StopInfo",
    "backend"
  ],
  "author": "nickbw2003",
  "license": "MIT",
  "jest": {
    "preset": "ts-jest"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/supertest": "^2.0.7",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "supertest": "^3.3.0",
    "ts-jest": "^23.10.5",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "@nestjs/common": "^5.5.0",
    "@nestjs/core": "^5.5.0",
    "@nestjs/microservices": "^5.5.0",
    "@nestjs/testing": "^5.5.0",
    "@nestjs/websockets": "^5.5.0",
    "dotenv": "^6.2.0",
    "public-transport-js": "^0.3.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3"
  }
}
